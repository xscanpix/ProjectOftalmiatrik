package com.anaxivis.application.fx.view;

import javafx.fxml.FXML;
import javafx.scene.image.Image;
import javafx.scene.image.ImageView;

import com.anaxivis.application.SVGImageLoader;
import com.anaxivis.application.fx.MainApp;

public class MainContentController {

    @FXML
    private ImageView imageView;

    private MainApp mainApp;

    public MainContentController() {
	
    }

    @FXML
    private void initialize() {
	imageView = new ImageView();
	imageView.setImage(SVGImageLoader.loadFromFile("Sloan2.svg"));
    }

    public void setMainApp(MainApp mainApp) {
	this.mainApp = mainApp;
    }

    public void setImage(Image value) {
	imageView.setImage(value);
    }
}
